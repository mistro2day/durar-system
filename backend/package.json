{
  "name": "durar-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec ts-node --esm src/index.ts",
    "seed": "ts-node prisma/seed.ts",
    "seed:admin": "ts-node prisma/seed-admin.ts",
    "build:client": "npm --prefix ../durar-dashboard run build",
    "dev:one": "npm run build:client && nodemon --exec ts-node --esm src/index.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.4.8",
    "nodemon": "^3.0.2",
    "prisma": "^6.17.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
