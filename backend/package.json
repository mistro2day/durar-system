{
  "name": "durar-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "seed": "ts-node prisma/seed.ts",
    "seed:admin": "ts-node prisma/seed-admin.ts",
    "build:client": "npm --prefix ../durar-dashboard run build",
    "dev:one": "npm run build:client && nodemon --exec ts-node --esm src/index.ts",
    "db:generate": "prisma generate",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:check": "node scripts/checkAdmin.cjs",
    "db:import:samples": "ts-node scripts/import-from-samples.ts",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.9",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.4.8",
    "@types/nodemailer": "^7.0.2",
    "nodemon": "^3.0.2",
    "prisma": "^6.17.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3"
  }
}
